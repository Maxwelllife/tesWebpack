<section class="container clients">

  <div class="d-none d-md-flex justify-content-between">
    <img src="./assets/tmp/logoopsum1.svg" alt="clients logo" class="d-inline-block w-20" />
    <img src="./assets/tmp/logoopsum2.svg" alt="clients logo" class="d-inline-block w-20" />
    <img src="./assets/tmp/logoopsum3.svg" alt="clients logo" class="d-inline-block w-20" />
    <img src="./assets/tmp/logoopsum4.svg" alt="clients logo" class="d-inline-block w-20" />
    <img src="./assets/tmp/logoopsum5.svg" alt="clients logo" class="d-inline-block w-20" />
  </div>

  <div class="d-md-none">
    <div id="carouselExampleDark" class="carousel carousel-dark slide">
      <div class="carousel-inner">

        <div class="carousel-item active" data-bs-interval="10000">
          <img src="./assets/tmp/logoopsum1.svg" alt="clients logo" class="d-block w-50" />
        </div>

        <div class="carousel-item" data-bs-interval="2000">
          <img src="./assets/tmp/logoopsum2.svg" alt="clients logo" class="d-block w-50" />
        </div>

        <div class="carousel-item">
          <img src="./assets/tmp/logoopsum3.svg" alt="clients logo" class="d-block w-50" />
        </div>

        <div class="carousel-item">
          <img src="./assets/tmp/logoopsum4.svg" alt="clients logo" class="d-block w-50" />
        </div>

        <div class="carousel-item ">
          <img src="./assets/tmp/logoopsum5.svg" alt="clients logo" class="d-block w-50" />
        </div>

      </div>
      <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleDark"
              data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
      </button>
      <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleDark"
              data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
      </button>
    </div>
  </div>
</section>
